{
  "name": "u-cms-client",
  "version": "1.0.0",
  "description": "CMS using Deno and Vue 3",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "build": "webpack --env.production",
    "check": "eslint src -c .eslintrc.js && npx prettier **/*.{js,ts,html,vue} --check && stylelint **/*.scss",
    "lint-fix": "eslint src -c .eslintrc.js --fix",
    "prettier-fix": "npx prettier . --write",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "test": "jest --passWithNoTests",
    "generate": "node_modules/vue-generator/generate.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint-fix && npm run prettier-fix && npm run stylelint-fix"
    }
  },
  "author": "Patryk Loter",
  "license": "ISC",
  "dependencies": {
    "@vue/runtime-dom": "^3.0.0-beta.15",
    "axios": "^0.19.2",
    "browser-cookies": "^1.2.0",
    "bulma": "^0.8.2",
    "query-string": "^6.13.1",
    "vue": "^3.0.0-beta.15",
    "vue-router": "^4.0.0-alpha.13",
    "vuex": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.8.3",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.12",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@vue/compiler-sfc": "^3.0.0-beta.15",
    "@vue/test-utils": "^2.0.0-alpha.6",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.3",
    "vue-generator": "git+https://github.com/Undero13/vue-generator.git",
    "vue-jest": "^5.0.0-alpha.0",
    "vue-loader": "^16.0.0-beta.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^4.0.0"
  },
  "peerDependencies": {
    "webpack": "^4.1.0 || ^5.0.0-0"
  }
}
