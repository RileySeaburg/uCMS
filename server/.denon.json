{
  "scripts": {
    "start": {
      "cmd": "index.ts",
      "desc": "Run backend app",
      "tsconfig": "tsconfig.json",
      "unstable": true,
      "watcher": {
        "interval": 500
      },
      "allow": [
        "all"
      ],
      "logger": {
        "debug": true
      },
      "fmt": true
    },
    "generate": {
      "cmd": "generateData.ts",
      "desc": "Genetare fake data for app",
      "unstable": true,
      "watch": false,
      "tsconfig": "tsconfig.json",
      "allow": [
        "all"
      ]
    },
    "test": {
      "cmd": "deno test --allow-all --unstable",
      "desc": "Run backend test",
      "watch": false
    }
  }
}